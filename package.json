{
  "name": "wtu",
  "version": "2.0.0-alpha",
  "description": "Extensible and scalable toolkit for templates prototyping",
  "author": "Maxim Belov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "node ./bin/www",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "build:dev": "cross-env NODE_ENV=development webpack --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --progress",
    "uikit-yarn-install": "cd ./node_modules/uikit && yarn install",
    "uikit-mkdir-icons": "mkdir -p ./node_modules/uikit/custom/icons",
    "uikit-cp-icons": "cp -R ./icons/. ./node_modules/uikit/custom/icons",
    "uikit-rm-icons": "rm -r ./node_modules/uikit/custom",
    "uikit-yarn-compile": "yarn --cwd ./node_modules/uikit run compile",
    "icons:build": "yarn run uikit-yarn-install && yarn run uikit-mkdir-icons && yarn run uikit-cp-icons && yarn run uikit-yarn-compile",
    "icons:rebuild": "yarn run uikit-rm-icons && yarn run uikit-mkdir-icons && yarn run uikit-cp-icons && yarn run uikit-yarn-compile",
    "imagemin": "imagemin ./imagemin/source/* --out-dir=./imagemin/optimized"
  },
  "dependencies": {
    "imask": "^6.2.2",
    "jquery": "^3.6.0",
    "uikit": "^3.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "expose-loader": "^3.1.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "imagemin-cli": "^7.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "morgan": "~1.9.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "tailwindcss": "^3.0.7",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
